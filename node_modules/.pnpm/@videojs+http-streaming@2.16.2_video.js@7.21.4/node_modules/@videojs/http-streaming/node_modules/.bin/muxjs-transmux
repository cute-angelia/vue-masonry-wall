#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vanilla/git/github/cute-angelia/vue-masonry-wall/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/vanilla/git/github/cute-angelia/vue-masonry-wall/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../mux.js@6.0.1/node_modules/mux.js/bin/transmux.js" "$@"
else
  exec node  "$basedir/../../../../../../mux.js@6.0.1/node_modules/mux.js/bin/transmux.js" "$@"
fi
